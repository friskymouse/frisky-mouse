<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFrameworks>net7.0-windows</TargetFrameworks>
		<Nullable>enable</Nullable>
		<UseWindowsForms>true</UseWindowsForms>
		<ImplicitUsings>enable</ImplicitUsings>
		<BaseOutputPath>..\bin</BaseOutputPath>
		<Platforms>AnyCPU;x64</Platforms>
		<LangVersion>10</LangVersion>
		<AssemblyVersion>1.0.0.1</AssemblyVersion>
		<Title>Frisky Mouse by FrostyBee</Title>
		<PackageId>FriskyMouse</PackageId>
		<Authors>FrostyBee</Authors>
		<Company />
		<Product>FriskyMouse</Product>
		<Copyright>FrostyBee</Copyright>
		<PackageIcon>Resources\bee-icon.png</PackageIcon>
		<Version>1.0.0.1</Version>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<ApplicationIcon>Resources\bee-icon.ico</ApplicationIcon>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<EnableUnmanagedDebugging>true</EnableUnmanagedDebugging>
		<RootNamespace>FriskyMouse</RootNamespace>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\FriskyMouse.Drawing\FriskyMouse.Drawing.csproj" />
		<ProjectReference Include="..\FriskyMouse.MaterialSkin\FriksyMouse.MaterialSkin.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="Properties\Resources.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>Resources.resx</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Update="Properties\Resources.resx">
			<Generator>ResXFileCodeGenerator</Generator>
			<LastGenOutput>Resources.Designer.cs</LastGenOutput>
		</EmbeddedResource>
	</ItemGroup>

	<ItemGroup>
		<None Remove="bee-icon.ico" />
		<None Remove="Resources\bee-icon.png" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="Resources\bee-icon.ico" />
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="Resources\bee-icon.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</EmbeddedResource>
	</ItemGroup>

	<ItemGroup>
		<Resource Include="bee-icon.ico">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</Resource>
	</ItemGroup>

	<ItemGroup>
		<Using Include="System.Drawing.Drawing2D" />
	</ItemGroup>

	<ItemGroup>
		<None Update="Resources\bee-icon.ico">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<EditorConfigFiles Remove="D:\Dev\my-repos\FriskyMouse\src\FriskyMouse\.editorconfig" />
	</ItemGroup>

	<ItemGroup>
		<None Include="D:\Dev\my-repos\FriskyMouse\src\FriskyMouse\.editorconfig" />
	</ItemGroup>

</Project>